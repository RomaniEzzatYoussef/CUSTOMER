    $INSERT I_EQUATE
    $INSERT I_F.CUSTOMER
	$INSERT I_MBSC.CUSTOMER.MINOR.COMMON
	$INSERT I_F.MBSC.MN.CUSTOMER
*-----------------------------------------------------------------------------



	FN.CUS = 'F.CUSTOMER'
