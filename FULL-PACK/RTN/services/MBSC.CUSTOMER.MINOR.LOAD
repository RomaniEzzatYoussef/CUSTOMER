    CALL OPF(FN.CUS,F.CUS)
	
	FN.MN.CUS = 'F.MBSC.MN.CUSTOMER'
    F.MN.CUS = ''
    CALL OPF(FN.MN.CUS,F.MN.CUS)


	FN.DATA = 'F.MBSC.MINOR.DATA'
    F.DATA = ''
    CALL OPF(FN.DATA,F.DATA)
	
	
	
	
*-----------------------------------------------------------------------------	
	
	   

RETURN
END